{
    "name": "ricardosierra/driver-discord",
    "license": "MIT",
    "description": "Discord driver for BotMan",
    "keywords": [
        "Bot",
        "BotMan",
        "Discord"
    ],
    "homepage": "http://github.com/botman/driver-discord",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "botman/botman": ">=2.0",
        "ricardosierra/discord-php": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Discord\\": "src/"
        }
    },
    "autoload-dev": {
